{
  "extends": ["config:base"],
  "enabledManagers": ["terraform"],
  "regexManagers": [
    {
      "fileMatch": ["modules/root/.*\\.tf$"],
      "matchStrings": [
        "source\\s*=\\s*\"git::https://github.com/VStefanov/tf.git//modules/(?<module>[^?]+)\\?ref=(?<currentValue>[^\"]+)\""
      ],
      "depNameTemplate": "tf-{{module}}",
      "datasourceTemplate": "git-tags",
      "versioningTemplate": "semver",
      "urlTemplate": "https://github.com/VStefanov/tf.git"
    }
  ]
}
