{
  "extends": [
    "config:base"
  ],
  "enabledManagers": [
    "terraform"
  ],
  "terraform": {
    "enabled": true
  },
  "ignoreUnstable": false,
  "regexManagers": [
    {
      "fileMatch": [
        "modules/root/.*\\.tf$"
      ],
      "matchStrings": [
        "source\\s*=\\s*\"git::https://github.com/VStefanov/tf.git//modules/(?<module>[^?]+)\\?ref=(?<currentValue>(?<prefix>\\w+)-v(?<version>\\d+\\.\\d+\\.\\d+))\""
      ],
      "depNameTemplate": "{{module}}-v{{version}}",
      "packageNameTemplate": "{{module}}-v{{version}}",
      "datasourceTemplate": "terraform-module"
    }
  ],
  "packageRules": [
    {
      "matchManagers": [
        "regex"
      ],
      "matchPackageNames": [
        "github.com/VStefanov/tf.git//modules/ec2"
      ],
      "extractVersion": "^ec2-v(?<version>\\d+\\.\\d+\\.\\d+)$"
    },
    {
      "matchManagers": [
        "regex"
      ],
      "matchPackageNames": [
        "github.com/VStefanov/tf.git//modules/rds"
      ],
      "extractVersion": "^rds-v(?<version>\\d+\\.\\d+\\.\\d+)$"
    },
    {
      "matchManagers": [
        "regex"
      ],
      "matchPackageNames": [
        "github.com/VStefanov/tf.git//modules/s3"
      ],
      "extractVersion": "^s3-v(?<version>\\d+\\.\\d+\\.\\d+)$"
    }
  ],
  "branchTopic": "{{{depNameSanitized}}}-{{{newVersion}}}",
  "prHourlyLimit": 0,
  "prConcurrentLimit": 2
}