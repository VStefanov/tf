{
  "extends": [
    "config:recommended"
  ],
  "enabledManagers": [
    "terraform"
  ],
  "terraform": {
    "enabled": true
  },
  "ignoreUnstable": false,
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/modules/root/.*\\.tf$/"
      ],
      "matchStrings": [
        "source\\s*=\\s*\"git::https://github.com/VStefanov/tf.git//modules/(?<module>[^?]+)\\?ref=(?<currentValue>(?<prefix>\\w+)-v(?<version>\\d+\\.\\d+\\.\\d+))\""
      ],
      "depNameTemplate": "{{{module}}}-v{{{version}}}",
      "packageNameTemplate": "{{{module}}}-v{{{version}}}",
      "datasourceTemplate": "terraform-module"
    }
  ],
  "branchTopic": "{{{depNameSanitized}}}-{{{newVersion}}}",
  "prHourlyLimit": 0,
  "prConcurrentLimit": 2
}